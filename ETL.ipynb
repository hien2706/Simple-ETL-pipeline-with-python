    "\n",
    "\n",
    "def load(dataframe, database_name, table_name):\n",
    "    \n",
    "    \n",
    "    conn_string = f'postgresql://{user}:{password}@{host}:{port}/{database_name}'\n",
    "    \n",
    "    engine = create_engine(conn_string)\n",
    "    conn = engine.connect() \n",
    "    dataframe.to_sql(table_name, con=conn, if_exists='replace', \n",
    "              index=True)\n",
    "    conn = psycopg2.connect(conn_string)\n",
    "    \n",
    "    conn.autocommit = True\n",
    "    cursor = conn.cursor() \n",
    "\n",
    "    #fetch data from inserted table\n",
    "    sql1 = f'''select * from {table_name};'''\n",
    "    cursor.execute(sql1) \n",
    "    loaded_df = pd.DataFrame(cursor.fetchall())\n",
    "    \n",
    "    logging.info(\"Begin DataFrame validation\\n\")\n",
    "    \n",
    "    try:\n",
    "        assert dataframe.shape[0] == loaded_df.shape[0]\n",
    "        assert dataframe.shape[1] == loaded_df.shape[1] - 1 # because added an Id column when inserting\n",
    "        print(f\"Success! The data in the {table_name} table have successfully been \"\n",
    "              f\"loaded and validated\")\n",
    "\n",
    "    except AssertionError:\n",
    "        logging.error(\"DataFrame shape is not consistent before and after loading!\")\n",
    "    \n",
    "    # conn.commit() \n",
    "    conn.close() \n",
    "    \n",
    "\n"
   ]
